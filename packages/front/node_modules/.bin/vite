#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/equinox/programming/ekeynox/node_modules/.pnpm/vite@5.4.7_@types+node@22.5.5_lightningcss@1.27.0_terser@5.33.0/node_modules/vite/bin/node_modules:/home/equinox/programming/ekeynox/node_modules/.pnpm/vite@5.4.7_@types+node@22.5.5_lightningcss@1.27.0_terser@5.33.0/node_modules/vite/node_modules:/home/equinox/programming/ekeynox/node_modules/.pnpm/vite@5.4.7_@types+node@22.5.5_lightningcss@1.27.0_terser@5.33.0/node_modules:/home/equinox/programming/ekeynox/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/equinox/programming/ekeynox/node_modules/.pnpm/vite@5.4.7_@types+node@22.5.5_lightningcss@1.27.0_terser@5.33.0/node_modules/vite/bin/node_modules:/home/equinox/programming/ekeynox/node_modules/.pnpm/vite@5.4.7_@types+node@22.5.5_lightningcss@1.27.0_terser@5.33.0/node_modules/vite/node_modules:/home/equinox/programming/ekeynox/node_modules/.pnpm/vite@5.4.7_@types+node@22.5.5_lightningcss@1.27.0_terser@5.33.0/node_modules:/home/equinox/programming/ekeynox/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/vite@5.4.7_@types+node@22.5.5_lightningcss@1.27.0_terser@5.33.0/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/vite@5.4.7_@types+node@22.5.5_lightningcss@1.27.0_terser@5.33.0/node_modules/vite/bin/vite.js" "$@"
fi
